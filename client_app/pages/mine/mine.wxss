
swiper{
  width: 100%;
  height: 400rpx;
}
swiper image{
  width: 100%;
  height: 100%;
}
.box{
  padding: 0 24rpx;
}
.box .title{
  text-align: center;
  width: 100%;
  border: 1rpx solid #cbcbcb;
  line-height: 80rpx;
  border-radius: 5rpx;
  margin: 30rpx auto;
  color: #a267c5;
}
.list{
  position: relative;
  padding: 50rpx 0;
  border-bottom:  1rpx solid #cbcbcb;
}
.list .head{
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
}
.name{
  position: absolute;
  left: 135rpx;
  top: 50rpx;
  font-weight: bold;
}
.number_ticket{
  position: absolute;
  left: 135rpx;
  top: 100rpx;
  font-size: 24rpx;
}
.number_mark{
  position: absolute;
  right: 0;
  top: 50%;
  -webkit-transform:translateY(-50%);
  color: #f50012;
  padding: 0 5rpx;
  font-size: 34rpx;
}
